  "version": "0.1.0",
  "devDependencies": {
    "babel-jest": "^26.3.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^5.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "jest": "^26.4.2",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "roots": [
      "app/javascript/test"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ]
  }
